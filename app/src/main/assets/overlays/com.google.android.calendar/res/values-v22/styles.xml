<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CalendarTheme.AllInOneActivity" parent="@*com.google.android.calendar:style/CalendarTheme.NoActionBar">
        <item name="android:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="android:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.calendar:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="*com.google.android.calendar:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
    </style>
    <style name="CalendarTheme.AllInOneActivity.LaunchScreen" parent="@*com.google.android.calendar:style/CalendarTheme.NoActionBar">
        <item name="android:windowBackground">@*com.google.android.calendar:drawable/quantum_launchscreen_calendar</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>

</resources>
