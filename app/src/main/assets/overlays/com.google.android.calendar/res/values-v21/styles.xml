<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBar" parent="@*android:style/Widget.Material.ActionBar.Solid" />
    <style name="ActionBar.Preferences" parent="@*com.google.android.calendar:style/ActionBar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:displayOptions">homeAsUp|showTitle</item>
    </style>

    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />

    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />

    <style name="BottomSheetActionButtonsThemeBase" parent="@*android:style/Widget.Material.Button.Borderless" />

    <style name="BundleActionBarV21">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:elevation">@*com.google.android.calendar:dimen/actionbar_elevation</item>
    </style>

    <style name="CalendarTheme.NoActionBar" parent="@*com.google.android.calendar:style/CalendarTheme.AbstractNoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:startYear">1970</item>
        <item name="android:endYear">2036</item>
        <item name="android:actionBarStyle">@*com.google.android.calendar:style/ActionBar.CustomView</item>
        <item name="android:colorEdgeEffect">@*android:color/accent_material_dark</item>
    </style>
    <style name="CalendarTheme.CreateGrooveActivity" parent="@*com.google.android.calendar:style/CalendarTheme.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="android:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.calendar:windowNoTitle">true</item>
        <item name="*com.google.android.calendar:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="*com.google.android.calendar:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
    </style>
    <style name="CalendarTheme.WithActionBar_v16" parent="@*com.google.android.calendar:style/CalendarTheme.NoActionBar">
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="android:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.calendar:windowActionBar">true</item>
        <item name="*com.google.android.calendar:windowNoTitle">false</item>
        <item name="*com.google.android.calendar:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="*com.google.android.calendar:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
    </style>


    <style name="DetailsThemeCommon" parent="@*com.google.android.calendar:style/CalendarTheme.Dialog">
        <item name="android:windowBackground">@*com.google.android.calendar:color/transparent_background_color</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:dropDownListViewStyle">@*com.google.android.calendar:style/NoDropDownDividers</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="android:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*com.google.android.calendar:color/details_status_bar_color</item>
        <item name="android:colorEdgeEffect">@*android:color/accent_material_dark</item>
        <item name="*com.google.android.calendar:windowActionBar">false</item>
        <item name="*com.google.android.calendar:windowNoTitle">true</item>
        <item name="*com.google.android.calendar:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="*com.google.android.calendar:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
    </style>

    <style name="FindTimeThemeBase" parent="@*com.google.android.calendar:style/CalendarTheme.AbstractNoActionBar">
        <item name="android:label">@*com.google.android.calendar:string/find_a_time_title</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:actionMenuTextAppearance">@*com.google.android.calendar:style/WhiteToolbarActionTextAppearance</item>
        <item name="android:colorEdgeEffect">@*android:color/accent_material_dark</item>
        <item name="*com.google.android.calendar:actionMenuTextColor">@*android:color/white</item>
        <item name="*com.google.android.calendar:colorAccent">@*com.google.android.calendar:color/quantum_googblue</item>
    </style>

    <style name="PreferencesThemeBase" parent="@*android:style/Theme.Material" />
    <style name="PreferencesTheme_v16" parent="@*com.google.android.calendar:style/PreferencesThemeBase">
        <item name="android:actionBarStyle">@*com.google.android.calendar:style/ActionBar.Preferences</item>
        <item name="android:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="android:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/accent_material_dark</item>
        <item name="*com.google.android.calendar:colorPrimary">@*com.google.android.calendar:color/calendar_primary</item>
        <item name="*com.google.android.calendar:colorAccent">@*com.google.android.calendar:color/accent_blue</item>
    </style>

    <style name="SearchToolbar" parent="@*com.google.android.calendar:style/Widget.AppCompat.Toolbar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:elevation">4.0dip</item>
        <item name="*com.google.android.calendar:actionBarSize">@*com.google.android.calendar:dimen/action_bar_height</item>
        <item name="*com.google.android.calendar:titleTextAppearance">@*com.google.android.calendar:style/TextAppearance.Toolbar.White</item>
        <item name="*com.google.android.calendar:navigationIcon">@*com.google.android.calendar:drawable/quantum_ic_arrow_back_grey600_24</item>
        <item name="*com.google.android.calendar:navigationContentDescription">@*com.google.android.calendar:string/accessibility_navigation_icon</item>
    </style>

    <style name="TextAppearance.AdvancedSearch_SpinnerBase" parent="@*android:style/Widget.Material.Spinner" />
    <style name="TextAppearance.EditEvent_SpinnerBase" parent="@*android:style/Widget.Material.Spinner" />
    <style name="TextAppearance.Recurrence_SpinnerButton" parent="@*android:style/Widget.Material.Spinner" />

    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.calendar:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.calendar:behavior_peekHeight">auto</item>
        <item name="*com.google.android.calendar:behavior_hideable">true</item>
        <item name="*com.google.android.calendar:behavior_skipCollapsed">false</item>
    </style>


    <style name="EventInfoActionBar" parent="@*android:style/ThemeOverlay.Material">
        <item name="android:actionOverflowButtonStyle">@*com.google.android.calendar:style/EventInfoActionBarOverflow</item>
    </style>
    <style name="EventInfoActionBarOverflow" parent="@*android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:src">@*com.google.android.calendar:drawable/ic_overflow_white_34</item>
    </style>

    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">@*com.google.android.calendar:color/abc_hint_foreground_material_dark</item>
        <item name="android:textColorHint">@*com.google.android.calendar:color/abc_hint_foreground_material_light</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>


    <style name="ActionbarTheme.Compat" parent="@*com.google.android.calendar:style/ActionbarTheme">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:displayOptions">homeAsUp|showTitle</item>
    </style>
    <style name="Theme.Actionbar.Base" parent="@*android:style/Widget.Material.ActionBar.Solid" />
    <style name="Theme.Activity.Light" parent="@*android:style/Theme.Material" />
</resources>
