<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@*android:color/ripple_material_dark"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/mask">
        <color android:color="@*android:color/button_material_dark" />
    </item>
    <item>
        <selector>
            <item android:state_enabled="true">
                <shape android:shape="rectangle">
                    <solid android:color="@*com.google.android.youtube:color/lc_button_style_primary_background" />
                    <corners android:radius="@*com.google.android.youtube:dimen/lc_button_corner_radius" />
                </shape>
            </item>
            <item android:state_enabled="false">
                <shape android:shape="rectangle">
                    <solid android:color="@*com.google.android.youtube:color/lc_button_style_disabled_background" />
                    <corners android:radius="@*com.google.android.youtube:dimen/lc_button_corner_radius" />
                </shape>
            </item>
        </selector>
    </item>
</ripple>
